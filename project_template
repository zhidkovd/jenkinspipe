#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@vm2 \'hostname\''
            }
    stages {
        stage("Secondary step") {
            steps {
                sh 'ssh root@vm2 \'ls\''
            }
    stages {
        stage("Secondary step") {
            steps {
                sh 'ssh root@vm2 \'echo Hello\''
            }
        }
    }
}
